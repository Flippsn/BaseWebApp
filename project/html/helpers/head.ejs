    apiKey: "<%= process.env.FIREBASE_API_KEY %>",
    authDomain: "webapp-hs.firebaseapp.com",
    databaseURL: "https://webapp-hs-default-rtdb.europe-west1.firebasedatabase.app",
    projectId: "webapp-hs",
    storageBucket: "webapp-hs.appspot.com",
    messagingSenderId: "1031811812385",
    appId: "1:1031811812385:web:d67ca1ee5a967959206760",
    measurementId: "G-012NXGDKL4"
  };
  // Initialize Firebase
  firebase.initializeApp(firebaseConfig);
  firebase.analytics();
</script>

<!-- then our app's own js file -->
<script src="js/main.js"></script>
